{
  "name": "scrabble-challenge",
  "version": "1.0.0",
  "description": "A module challenge",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "watch-sass": "sass src/styles/sass-main.scss:src/styles/sass.css --watch",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "npx vitest",
    "test-all": "echo \"to be defined\" && exit 1",
    "test-extensions": "echo \"to be defined\" && exit 1",
    "prepare": "husky install"
  },
  "author": "rafa-lopes",
  "license": "ISC",
  "prettier": "@boolean-uk/boolean-prettier-config",
  "dependencies": {
    "antd": "^5.14.2",
    "dayjs": "^1.11.10",
    "framer-motion": "^11.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@boolean-uk/boolean-prettier-config": "^1.0.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^7.0.4",
    "sass": "^1.71.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vitest": "^1.3.1"
  }
